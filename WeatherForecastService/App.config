        <endpoint address="" binding="netTcpBinding" contract="FTN.ServiceContracts.ISCADAForwarding" bindingConfiguration="netTcpBindingNoSecurity">
          <identity>
            <dns value="localhost"/>
          </identity>
        </endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:10011/ISCADAForwarding"/>
          </baseAddresses>
        </host>
      </service>
    </services>-->
	</system.serviceModel>

</configuration>